programa
{
	
	funcao inicio()
	{		inteiro num1
		escreva("digiite um numero: ")
		leia(num1)
		se(num1 > 0){
		escreva("numero positivo ")	
		}senao se( num1 == 0){
			escreva("numero neltro")
		} senao {
			escreva(" numero negativo ")
		}
				
	}
}
